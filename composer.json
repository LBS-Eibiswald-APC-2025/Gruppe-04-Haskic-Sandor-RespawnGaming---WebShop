        "gregwar/captcha": "~1.1",
        "phpunit/phpunit": "4.8.*|5.7.*",
        "paypal/paypal-server-sdk": "0.6.1",
        "paypal/paypal-checkout-sdk": "1.0.2",
        "fpdf/fpdf": "^1.86"
    },
    "autoload": {
        "psr-4": { "": ["application/core/", "application/model/"] }
